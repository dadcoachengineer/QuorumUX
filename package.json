{
  "name": "quorum-ux",
  "version": "0.3.0",
  "description": "Multi-model consensus UX analysis from E2E test artifacts",
  "type": "module",
  "bin": {
    "quorumux": "dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "ux",
    "testing",
    "ai",
    "multi-model",
    "visual-testing",
    "playwright",
    "accessibility",
    "e2e",
    "openrouter",
    "claude",
    "gemini",
    "gpt4",
    "consensus"
  ],
  "author": "Jason Shearer <jason@momentumeq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadcoachengineer/QuorumUX.git"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "tsx": "^4.19.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  }
}
